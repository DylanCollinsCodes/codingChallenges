var twoSum = function(nums, target) {
    for(i = 0; i < nums.length; i++){
        for(j = 0; j < nums.length; j++){
            if(j === i){
                j++
            }
            value = nums[i] + nums[j]
            if(value === target){
                return arr = [i, j]
            }
        } 
    }
};
